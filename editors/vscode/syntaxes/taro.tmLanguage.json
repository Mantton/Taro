{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "Taro",
    "scopeName": "source.taro",
    "patterns": [
        {
            "include": "#comments"
        },
        {
            "include": "#strings"
        },
        {
            "include": "#keywords"
        },
        {
            "include": "#constants"
        },
        {
            "include": "#storage"
        },
        {
            "include": "#functions"
        },
        {
            "include": "#types"
        },
        {
            "include": "#attributes"
        },
        {
            "include": "#numbers"
        },
        {
            "include": "#operators"
        }
    ],
    "repository": {
        "comments": {
            "patterns": [
                {
                    "name": "comment.line.double-slash.taro",
                    "match": "//.*$"
                },
                {
                    "name": "comment.block.taro",
                    "begin": "/\\*",
                    "end": "\\*/"
                }
            ]
        },
        "strings": {
            "patterns": [
                {
                    "name": "string.quoted.double.taro",
                    "begin": "\"",
                    "end": "\"",
                    "patterns": [
                        {
                            "name": "constant.character.escape.taro",
                            "match": "\\\\."
                        }
                    ]
                },
                {
                    "name": "string.quoted.single.taro",
                    "begin": "'",
                    "end": "'",
                    "patterns": [
                        {
                            "name": "constant.character.escape.taro",
                            "match": "\\\\."
                        }
                    ]
                }
            ]
        },
        "keywords": {
            "patterns": [
                {
                    "name": "keyword.control.taro",
                    "match": "\\b(if|else|for|while|loop|break|continue|return|defer|guard|match|case|in|where)\\b"
                },
                {
                    "name": "keyword.other.taro",
                    "match": "\\b(import|export|extern|namespace|as|any)\\b"
                },
                {
                    "name": "keyword.operator.taro",
                    "match": "\\b(operator)\\b"
                },
                {
                    "name": "keyword.operator.new.taro",
                    "match": "\\b(extend)\\b"
                }
            ]
        },
        "storage": {
            "patterns": [
                {
                    "name": "storage.type.taro",
                    "match": "\\b(func|struct|enum|interface|type)\\b"
                },
                {
                    "name": "storage.type.reserved.taro",
                    "match": "\\b(class)\\b"
                },
                {
                    "name": "storage.modifier.taro",
                    "match": "\\b(public|private|static|readonly|mut|const|var|let)\\b"
                },
                {
                    "name": "storage.modifier.reserved.taro",
                    "match": "\\b(final|override|fileprivate|protected|async|await|ref|init)\\b"
                }
            ]
        },
        "constants": {
            "patterns": [
                {
                    "name": "constant.language.taro",
                    "match": "\\b(true|false|nil)\\b"
                },
                {
                    "name": "variable.language.this.taro",
                    "match": "\\b(self)\\b"
                },
                {
                    "name": "support.type.self.taro",
                    "match": "\\bSelf\\b"
                }
            ]
        },
        "functions": {
            "patterns": [
                {
                    "match": "\\b(func)\\s+([a-zA-Z_][a-zA-Z0-9_]*)",
                    "captures": {
                        "1": {
                            "name": "storage.type.function.taro"
                        },
                        "2": {
                            "name": "entity.name.function.taro"
                        }
                    }
                },
                {
                    "match": "\\b(operator)\\s+([a-zA-Z_0-9!%^&|*+\\-=<>/?]+)",
                    "captures": {
                        "1": {
                            "name": "keyword.operator.taro"
                        },
                        "2": {
                            "name": "entity.name.function.operator.taro"
                        }
                    }
                },
                {
                    "name": "entity.name.function.call.taro",
                    "match": "\\b([a-zA-Z_][a-zA-Z0-9_]*)(?=\\()"
                },
                {
                    "name": "support.function.builtin.taro",
                    "match": "\\b(print|println|eprint|assert|panic|make|len|cap|append|push|pop|remove|insert)\\b"
                }
            ]
        },
        "types": {
            "patterns": [
                {
                    "name": "entity.name.type.taro",
                    "match": "\\b[A-Z][a-zA-Z0-9_]*\\b"
                },
                {
                    "name": "support.type.primitive.taro",
                    "match": "\\b(bool|string|rune|void|any|isize|usize|int8|int16|int32|int64|uint8|uint16|uint32|uint64|float|double)\\b"
                },
                {
                    "name": "support.type.builtin.taro",
                    "match": "\\b(Option|Result|List|Dictionary|Array|Set|Clone|Copy|Hashable|Equatable|GcPtr|Range|ClosedRange)\\b"
                }
            ]
        },
        "attributes": {
            "name": "meta.attribute.taro",
            "match": "@[a-zA-Z_][a-zA-Z0-9_]*\\b"
        },
        "numbers": {
            "patterns": [
                {
                    "name": "constant.numeric.binary.taro",
                    "match": "\\b0b[01_]+\\b"
                },
                {
                    "name": "constant.numeric.octal.taro",
                    "match": "\\b0o[0-7_]+\\b"
                },
                {
                    "name": "constant.numeric.hex.taro",
                    "match": "\\b0x[a-fA-F0-9_]+\\b"
                },
                {
                    "name": "constant.numeric.decimal.taro",
                    "match": "\\b[0-9][0-9_]*(\\.[0-9][0-9_]*)?([eE][+-]?[0-9_]+)?\\b"
                }
            ]
        },
        "operators": {
            "patterns": [
                {
                    "name": "keyword.operator.comparison.taro",
                    "match": "(===|==|!=|<=|>=)"
                },
                {
                    "name": "keyword.operator.logical.taro",
                    "match": "(&&|\\|\\|)"
                },
                {
                    "name": "keyword.operator.assignment.compound.taro",
                    "match": "(\\+=|\\-=|\\*=|\\/=|%=|&=|\\|=|\\^=|<<=|>>=)"
                },
                {
                    "name": "keyword.operator.bitwise.taro",
                    "match": "(<<|>>)"
                },
                {
                    "name": "keyword.operator.arrow.taro",
                    "match": "(\\->|=>)"
                },
                {
                    "name": "keyword.operator.range.taro",
                    "match": "(\\.\\.\\.|\\.\\.=|\\.\\.)"
                },
                {
                    "name": "keyword.operator.optional.taro",
                    "match": "(\\?\\.|\\?\\?)"
                },
                {
                    "name": "keyword.operator.pipe.taro",
                    "match": "\\|>"
                },
                {
                    "name": "keyword.operator.dotstar.taro",
                    "match": "\\.\\*"
                },
                {
                    "name": "keyword.operator.taro",
                    "match": "(\\+|\\-|\\*|\\/|%|\\^|&|\\||!|~|<|>|=|\\?|\\.|:)"
                }
            ]
        }
    }
}