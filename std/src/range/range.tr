/// A half-open range from `start` to `end` (exclusive).
/// Created with the `..` operator: `start..end`
public struct Range[Bound] {
    start: Bound
    end: Bound
}

/// A closed range from `start` to `end` (inclusive).
/// Created with the `..=` operator: `start..=end`
public struct ClosedRange[Bound] {
    start: Bound
    end: Bound
}
