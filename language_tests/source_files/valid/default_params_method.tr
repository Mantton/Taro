struct Counter {
    base: int32
}

impl Counter {
    func add(&self, value: int32 = 10) -> int32 {
        return self.base + value
    }
}

func main() {
    let counter = Counter { base: 7 }

    if counter.add() != 17 {
        std.print("FAIL: counter.add() default failed\n")
    }

    if counter.add(value: 5) != 12 {
        std.print("FAIL: counter.add(value: 5) failed\n")
    }

    std.print("PASS: Default params on methods\n")
}
