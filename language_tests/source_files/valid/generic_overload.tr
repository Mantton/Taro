// Test: Generic Overload Resolution
// Checks that more specific function overloads are preferred over generic ones.

func main() {
    let a: uint32 = 10
    let b = use(value: a)
    
    assert(b == 10, "Value mismatch")
    
    // Value check ensures a valid overload was selected.
    
    std.print("PASS: Generic overload resolution\n")
}

func use[V](value: V) -> V { value }
func use(value: uint32) -> uint32 { value }
