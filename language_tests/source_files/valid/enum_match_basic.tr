// Test: Enum Matching Basic
// Checks basic pattern matching on enum variants with and without associated values.

func main() {
    let a = Entity.unit
    let b = Entity.tuple(10, 20)
    let c = Entity.unit

    match a {
        case Entity.unit => {
            std.print("PASS: Match unit\n")
        }
        case Entity.tuple(a, b) => {
            assert(false, "Should not match tuple")
        }
    }

    match b {
        case Entity.unit => {
            assert(false, "Should not match unit")
        }
        case Entity.tuple(x, y) => {
            assert(x == 10, "First element mismatch")
            assert(y == 20, "Second element mismatch")
            std.print("PASS: Match tuple\n")
        }
    }
}

enum Entity {
    case unit
    case tuple(usize, usize)
}
