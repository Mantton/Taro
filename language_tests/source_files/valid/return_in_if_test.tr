// Minimal reproduction - using local variable in function with return in if

import std.optional.Optional

func test_with_local(current: int32, end: int32) -> Optional[int32] {
    if current < end {
        let value = current
        return Optional.some(value)
    }
    return Optional.none
}

func main() {
    let result = test_with_local(current: 0, end: 5)
}
