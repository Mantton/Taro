// TARGET: aarch64-apple-darwin
// CHECK_ONLY
// Test #cfg() compile-time expression syntax
// Verifies that #cfg(os("macos")) evaluates to a boolean at compile time

func main() {
    // Simple os check
    if #cfg(os("macos")) {
        print("Running on macOS\n")
    }
    
    // Negation
    if #cfg(!os("windows")) {
        print("Not running on Windows\n")
    }
    
    // AND operator
    if #cfg(os("macos") && arch("aarch64")) {
        print("Running on Apple Silicon\n")
    }
    
    // OR operator  
    if #cfg(os("macos") || os("linux")) {
        print("Running on Unix-like OS\n")
    }
    
    // Complex expression
    if #cfg(!os("windows") && (os("macos") || os("linux"))) {
        print("Definitely not Windows, and Unix-like\n")
    }
    
    print("#cfg expression test passed!\n")
}
