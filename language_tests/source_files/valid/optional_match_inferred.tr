// Test: Optional Match Inferred
// Checks matching on Optionals using inferred case paths (.some/.none).

import std.prelude.Optional

func main() {
    let a: Optional[string] = Optional.some("Hello, World")

    match a {
        case .none => {
            assert(false, "Should be some")
        }
        case .some(str) => {
            assert(true, "Matched inferred .some")
        }
    }
    
    std.print("PASS: Inferred optional match working\n")
}
