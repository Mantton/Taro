@cfg(profile("debug"))
func profileName() -> string {
    "debug"
}

@cfg(profile("release"))
func profileName() -> string {
    "release"
}

func main() {
    assert(#cfg(profile("debug")), "expected debug profile")
    assert(!#cfg(profile("release")), "release cfg should be false in debug profile")

    assert(profileName() == "debug", "profile-selected declaration should be debug variant")
    debugAssert(true, "debug assert should not panic")

    print("PASS: debug assert profile cfg\n")
}
