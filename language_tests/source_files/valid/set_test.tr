// TEST

@test
func setInsertContainsRemove() {
    var set: Set[int32] = Set()
    std.testing.assertTrue(set.isEmpty(), "new set should start empty")
    std.testing.assertEqual(set.len(), 0 as usize, "new set len should be zero")

    set.insert(7)
    set.insert(7)
    set.insert(8)

    std.testing.assertTrue(set.contains(7), "set should contain inserted value")
    std.testing.assertTrue(set.contains(8), "set should contain second inserted value")
    std.testing.assertFalse(set.contains(9), "set should not contain missing value")
    std.testing.assertEqual(set.len(), 2 as usize, "set should deduplicate inserts")

    let removed7 = set.remove(7)
    std.testing.assertTrue(removed7, "remove should return true for present value")
    std.testing.assertFalse(set.contains(7), "removed value should not be present")
    std.testing.assertEqual(set.len(), 1 as usize, "set len should decrement after remove")

    let removedMissing = set.remove(42)
    std.testing.assertFalse(removedMissing, "remove should return false for missing value")
}

func main() {}
