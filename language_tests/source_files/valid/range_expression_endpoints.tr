func lower() -> int32 { 1 }
func upper() -> int32 { 6 }

func main() {
    var sum = 0
    for n in (lower() + 1)..(upper() - 1) {
        sum = sum + n
    }
    assert(sum == 9, "(1+1)..(6-1) should yield 2,3,4")

    let a: int32 = 4
    let b: int32 = 8
    var count = 0
    for _ in (a - 2)..=(b - 6) {
        count = count + 1
    }
    assert(count == 1, "(4-2)..=(8-6) should be a singleton")

    print("Range expression endpoints ok\n")
}
