// Test: Function declarations and calls

func noParams() -> int32 {
    return 42
}

func oneParam(x: int32) -> int32 {
    return x * 2
}

func twoParams(a: int32, b: int32) -> int32 {
    return a + b
}

func voidReturn() {
    let x = 10
}

func main() {
    let a = noParams()
    let b = oneParam(x: 10)
    let c = twoParams(a: 5, b: 3)
    voidReturn()
}
