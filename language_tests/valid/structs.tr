// Test: Struct definitions and usage

struct Point {
    x: int32
    y: int32
}

struct Rectangle {
    origin: Point
    width: int32
    height: int32
}

func makePoint(x: int32, y: int32) -> Point {
    return Point { x: x, y: y }
}

func main() {
    let p = makePoint(x: 10, y: 20)
    let r = Rectangle {
        origin: p,
        width: 100,
        height: 50,
    }
}
